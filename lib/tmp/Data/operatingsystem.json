{
  "Android": {
    "Android": {
      "vendor": "Google",
      "model": "Android",
      "matchIdentity": [
        {
          "context": null,
          "match": [
            "Android"
          ],
          "matchType": "strpos"
        }
      ],
      "matchVersion": [
        {
          "context": null,
          "match": [
            "Android[ -][VER]"
          ],
          "matchType": "regex",
          "matchProcessor": "getAndroidVersions"
        }
      ],
      "triggers": [
        {
          "isMobile": true
        }
      ]
    }
  },
  "iOS": {
    "iOS": {
      "vendor": "Apple",
      "model": "iOS",
      "matchIdentity": [
        {
          "context": null,
          "match": [
            "\\biPhone.*(Mobile|OS)|\\biPod|\\biPad"
          ],
          "matchType": "regex"
        }
      ],
      "matchVersion": [
        {
          "context": null,
          "match": [
            "\\bOS\\b [VER]",
            "iPhone OS\/[VER]",
            "iOS [VER];",
            "Version\/[VER]"
          ],
          "matchProcessor": "getiOsVersion"
        }
      ],
      "triggers": [
        {
          "isMobile": true
        }
      ]
    }
  },
