  "BlackBerry": {
    "vendor": "BlackBerry",
    "matchIdentity": [
      {
        "context": null,
        "match": [
          "PlayBook|RIM Tablet"
        ],
        "matchType": "regex"
      }
    ],
    "matchModelAndVersion": false
  },
  "Acer": {
    "readme": "https://github.com/serbanghita/Mobile-Detect/wiki/tablet.json#acer",
    "vendor": "Acer",
    "matchIdentity": [
      {
        "context": null,
        "match": [
          "Android.*; \\b(A100|A101|A110|A200|A210|A211|A500|A501|A510|A511|A700|A701|W500|W500P|W501",
