{
  "iPad": {
    "vendor": "Apple",
    "model": "iPad",
    "matchIdentity": [
      {
        "context": null,
        "match": [
          "iPad|iPad.*Mobile"
        ],
        "matchType": "regex"
      }
    ],
    "matchModelAndVersion": [
      {
        "context": null,
        "match": "iPad.*CPU[a-z ]+(?<version>[0-9\\._-]+)",
        "matchType": "regex"
      }
    ]
  },
  "Nexus": {
    "vendor": "Google",
    "matchIdentity": [
      {
        "context": null,
        "match": "Android.*Nexus[\\s]+(7|9|10)|^.*Android.*Nexus(?:(?!Mobile).)*$",
        "matchType": "regex"
      }
    ],
    "matchModelAndVersion": [
      {
        "context": null,
        "match": [
          "Google (?<model>[a-zA-Z]+ (?<version>[a-zA-Z0-9]+))",
          "(?<model>Nexus (?<version>[a-zA-Z0-9]+)) Build",
          "(?<model>[a-zA-Z0-9]+ Nexus)"
        ],
        "matchType": "regex"
      }
    ]
  },
  "Samsung": {
