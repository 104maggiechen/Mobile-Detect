    ],
    "matchModelAndVersion": [
      {
        "context": null,
        "match": [
          "; (?<model>[a-zA-Z0-9-]+) Build",
          "; Acer; (?<model>[a-zA-Z0-9-]+)"
        ]
      }
    ]
